
@fib:
    blt.i64 %1 2 [fib.then] [fib.else]
@fib.then:
    ret.i64 %1
@fib.else:
    sub.i64 %0 %1 2
    call %0 [fib] %0
    sub.i64 %1 %1 1
    call %1 [fib] %1
    add.i64 %0 %0 %1
    ret.i64 %0

@putn:
    blt.i64 %1 1 [putn.ret] [putn.digit]
@putn.digit:
    div.i64 %0 %1 10
    call %0 [putn] %0
    mod.i64 %1 %1 10
    add.i64 %1 %1 48
    out.i64 %1
    jump [putn.ret]
@putn.ret:
    ret.i64 0

@:
    move.i64 %0 5
    call %0 [fib] %0
    call %0 [putn] %0
    out.i64 10
    exit
