
do  set count 1i64
do  set max 1000i64
do  set pprime 3i64
do  while lt pprime max
    do  set check 3i64
    do  set isprime 1i64
    do  while le mul check check pprime
            do  when eq mod pprime check 0i64
                    set isprime 0i64
                set check add check 2i64
    do  set count add count isprime
        set pprime add pprime 2i64
    return count
