
do  set count 1i32
do  set max 1000000i32
do  set pprime 3i32
do  while lt pprime max
    do  set check 3i32
    do  set isprime 1i32
    do  while le mul check check pprime
            do  when eq mod pprime check 0i32
                    set isprime 0i32
                set check add check 2i32
    do  set count add count isprime
        set pprime add pprime 2i32
    return count
