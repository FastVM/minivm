
@fib:
    blt %1 2 [fib.then] [fib.else]
@fib.then:
    ret %1
@fib.else:
    sub %0 %1 2
    call %0 [fib] %0
    sub %1 %1 1
    call %1 [fib] %1
    add %0 %0 %1
    ret %0

@putn:
    blt %1 1 [putn.ret] [putn.digit]
@putn.digit:
    div %0 %1 10
    call %0 [putn] %0
    mod %1 %1 10
    add %1 %1 48
    out %1
    jump [putn.ret]
@putn.ret:
    ret 0

@:
    move %0 15
    call %0 [fib] %0
    call %0 [putn] %0
    out 10
    exit
