@putn:
    blt %1 1 [putn.ret] [putn.digit]
@putn.digit:
    div %0 %1 10
    call %0 [putn] %0
    mod %1 %1 10
    add %1 %1 48
    out %1
@putn.ret:
    ret 0

@print:
    bb %1 [print.nonzero] [print.zero]
@print.zero:
    out 48
    out 10
    ret 0
@print.nonzero:
    call %0 [putn] %1
    out 10
    ret 0

@:
    move.i32 %0 8
    cast.i16 %0 %0
    call %0 [print] %0
    move %0 16
    exit
