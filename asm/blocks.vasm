@fib:
    blt %1 2 {
        ret %1
    } {
        sub %0 %1 1
        call %1 [fib] %0 
        sub %0 %0 1
        call %0 [fib] %0
        add %0 %0 %1
        ret %0
    }

@putn:
    blt %1 1 {
        ret 0
    } {
        div %0 %1 10
        call %0 [putn] %0
        mod %1 %1 10
        add %1 %1 48
        out %1
        ret 0
    }

@:
    move.u32 %0 35
    call %0 [fib] %0
    call %0 [putn] %0
    exit 