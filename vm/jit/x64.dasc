                | mov edi, ((int32_t) (instr.args[0].num))
            } else {
                __builtin_trap();
            }
            | mov64 rax, ((uint64_t) &vm_print)
            | call rax
            {
                size_t count = 0;
                for (size_t i = 0; i < 16; i++) {
                    if (instr.out.save & (1 << i)) {
                        | mov Rq(i), [rsp + (8 * count + 8)]
