                        | mul Rq(r)
                        if (instr.out.x64 != out) {
                            | mov Rq(instr.out.x64), Rq(out)
                        }
                    } else {
                        __builtin_trap();
                    }
                } else if (instr.args[0].type == VM_ARG_NUM) {
                    if (instr.args[1].type == VM_ARG_X64) {
                        __builtin_trap();
                    } else if (instr.args[1].type == VM_ARG_NUM) {
                        __builtin_trap();
                    } else {
                        __builtin_trap();
                    }
                } else {
                    __builtin_trap();
                }
                if (instr.out.save & (1 << 0)) {
                    | mov rax, [rsp+8]
                }
                if (instr.out.save & (1 << 2)) {
                    | mov rdx, [rsp+16]
                }
